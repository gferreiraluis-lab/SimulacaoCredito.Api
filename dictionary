TABLE `chathistory` (
  `Sequence_ID` bigint NOT NULL AUTO_INCREMENT,
  `Flow_Sequence_ID` bigint DEFAULT NULL,
  `Step_Index_ID` bigint DEFAULT NULL,
  `Tel_Client_ID` varchar(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci DEFAULT NULL,
  `Agent_ID` varchar(100) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci DEFAULT NULL,
  `Empresa_ID` int DEFAULT NULL,
  `Channel_ID` int DEFAULT NULL,
  `UpdateDateTime` datetime(3) DEFAULT NULL,
  `Incoming` int DEFAULT NULL,
  `Message` longtext CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci,
  `Protocol` char(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci DEFAULT NULL,
  `ChatUUID` char(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci DEFAULT NULL,
  `HermesTimeStamp` datetime(3) DEFAULT NULL,
  `Response_Interval` int DEFAULT NULL,
  `MessageId` varchar(256) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci DEFAULT NULL,
  `MessageContextId` varchar(256) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci DEFAULT NULL,
  `Step_Name` varchar(600) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci DEFAULT NULL,
  `JsonExtras` longtext COLLATE utf8mb4_general_ci,
  `UUID` varchar(100) COLLATE utf8mb4_general_ci DEFAULT NULL,
  PRIMARY KEY (`Sequence_ID`),
  KEY `chathistory_ChatUUID_IDX` (`ChatUUID`) USING BTREE,
  KEY `chathistory_UpdateDateTime_IDX` (`UpdateDateTime`) USING BTREE
) 
